<div class = "upcoming" >
    <h1>Upcaming events</h1>
<% @upcoming_events.each do |e| %>
  
      <ul>
            <li><%= e.title %></li>
            <li><%= e.description %></li>
            <li><%= e.date %></li>
            <li><%= e.location %></li>
      </ul>
  
<% end %>
</div>
<div class = "past events" >
    <h1>Past events</h1>
<% @past_events.each do |e| %>
      <ul>
            <li><%= e.title %></li>
            <li><%= e.description %></li>
            <li><%= e.date %></li>
            <li><%= e.location %></li>
      </ul>
<% end %>